{
  "name": "db-prisma",
  "version": "0.1.0",
  "description": "Testing stacks with todo apps",
  "repository": "https://github.com/vadistic/todo",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint src.{ts,tsx} --fix",
    "build": "tsc -p tsconfig.json",
    "start": "echo TODO",
    "test": "jest",
    "generate": "prisma2 generate",
    "postinstall": "npm -s run generate",
    "migrate": "yarn migrate:save && yarn migrate:up",
    "migrate:save": "prisma2 migrate save --experimental",
    "migrate:up": "prisma2 migrate up --experimental"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview024"
  },
  "devDependencies": {
    "@vadistic/eslint-config-node": "^0.1.8",
    "@vadistic/prettier-config": "^0.1.8",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "prisma2": "^2.0.0-preview024",
    "typescript": "^3.8.3"
  }
}
