    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.2",
    "@types/convict": "^4.2.1",
    "@types/sqlite3": "^3.1.6",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0"
  }
