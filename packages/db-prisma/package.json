{
  "name": "@todo/db-prisma",
  "version": "0.1.0",
  "private": true,
  "description": "Testing stacks with todo apps",
  "repository": "https://github.com/vadistic/todo",
  "license": "MIT",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "bootstrap": "yarn scripts bootstrap",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "copyfiles -u 1 ./src/generated/client/** dist",
    "clean": "rm -rf dist *.tsbuildinfo",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate": "prisma generate",
    "lint": "eslint --ext ts,tsx src/**",
    "scripts": "sh ./scripts.sh",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.1",
    "@todo/shared-db": "^0.1.0",
    "convict": "^5.2.0",
    "sqlite": "^4.0.0-beta.7",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.1",
    "@types/convict": "^4.2.1",
    "@types/sqlite3": "^3.1.6",
    "@vadistic/eslint-config-base": "^0.2.2",
    "@vadistic/prettier-config": "^0.2.2",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  }
}
