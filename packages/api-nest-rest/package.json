{
  "name": "@todo/api-nest-rest",
  "version": "0.1.0",
  "private": true,
  "description": "Testing stacks with todo apps",
  "repository": "https://github.com/vadistic/todo",
  "license": "MIT",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "format": "prettier --write **/*.ts",
    "lint": "eslint --ext ts,tsx src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "scripts": "sh ./scripts.sh",
    "start": "nest start -p tsconfig.build.json",
    "start:debug": "nest start -p tsconfig.build.json --debug --watch",
    "start:dev": "nest start -p tsconfig.build.json --watch",
    "start:prod": "node dist/main"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/config": "^0.4.1",
    "@nestjs/core": "^7.0.9",
    "@nestjs/mongoose": "^7.0.0",
    "@nestjs/platform-express": "^7.0.9",
    "convict": "^6.0.0",
    "mongoose": "^5.9.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.9",
    "@types/convict": "^5.2.0",
    "@types/mongoose": "^5.7.15"
  }
}
